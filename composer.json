{
  "name": "andreyors/behat-bootstrap",
  "type": "behat-extension",
  "description": "A Behat extension to automate init actions",
  "keywords": ["symfony", "behat", "bootstrap"],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Orsoev",
      "email": "andrey.orsoev@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "AndreyOrs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\AndreyOrs\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.3.3",
    "behat/behat": "^3.4.3",
    "symfony/config": "~3.2||~4.0",
    "symfony/dependency-injection": "~3.2||~4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "phake/phake": "^2.3 || ^3.0",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.3"
  }
}
